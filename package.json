{
  "name": "react-sparklines",
  "version": "1.5.2",
  "description": "Beautiful and expressive Sparklines component for React ",
  "main": "build/index.js",
  "directories": {
    "src": "src/"
  },
  "scripts": {
    "start": "cd demo && webpack-dev-server --progress",
